      <Pane fx:id="backgroundPane" prefHeight="550.0" prefWidth="950.0" />
      <Pane fx:id="HomeBackgroundPane" layoutX="48.0" layoutY="25.0" prefHeight="500.0" prefWidth="850.0">
         <children>
            <Pane fx:id="logoPane" layoutX="350.0" layoutY="-23.0" prefHeight="150.0" prefWidth="150.0" />
            <ImageView fx:id="LogoHomeDashboard" fitHeight="150.0" fitWidth="200.0" layoutX="350.0" layoutY="-27.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../Graphics/Sportunus_logo.png" />
               </image>
            </ImageView>
            <Button fx:id="Contactus" layoutX="500.0" layoutY="288.0" mnemonicParsing="false" onAction="#ContactUsTransition" text="Contactez nous" />
            <Button fx:id="FacebookPage" layoutX="259.0" layoutY="288.0" mnemonicParsing="false" onAction="#FacebookTransition" text="Page Facebook" />
         </children>
      </Pane>
   </children>
